{
  "name": "gdquest",
  "version": "0.0.1",
  "homepage": "https://xananax.github.io/svelte-kit",
  "repository": {
    "url": "https://github.com/Xananax/svelte-kit"
  },
  "scripts": {
    "dev": "svelte-kit dev",
    "build:site": "svelte-kit build",
    "build:build-scripts": "npx tsc --project build-scripts/tsconfig.json",
    "watch:build-scripts": "npx tsc --project build-scripts/tsconfig.json --watch",
    "build:products": "node ./build-scripts/products-creator/index.js",
    "build": "run-s clean:build build:site",
    "disable-jekill": "touch build/.nojekyll",
    "deploy:gh-pages": "gh-pages -d build -t true",
    "deploy": "run-s build disable-jekill deploy:gh-pages",
    "preview": "svelte-kit preview",
    "check:ts": "svelte-check --tsconfig ./tsconfig.json",
    "check:ts:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "check:lint": "eslint --ignore-path .gitignore .",
    "check:pretty": "prettier --check --plugin-search-dir=. .",
    "check": "run-s check:lint check:pretty",
    "format": "prettier --write --plugin-search-dir=. .",
    "clean:svelte": "rimraf .svelte-kit",
    "clean:build": "rimraf build",
    "clean:modules": "rimraf node_modules",
    "clean": "run-s clean:svelte clean:build clean:modules",
    "postinstall": "run-s build:build-scripts"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0-next.43",
    "@sveltejs/adapter-static": "^1.0.0-next.16",
    "@sveltejs/kit": "1.0.0-next.147",
    "@types/cookie": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "gh-pages": "^3.2.3",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.34.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "type": "module",
  "dependencies": {
    "@lukeed/uuid": "^2.0.0",
    "@stripe/stripe-js": "^1.17.1",
    "@types/youtube": "^0.0.44",
    "cookie": "^0.4.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.6",
    "mdsvex": "^0.9.8",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "pug": "^3.0.2",
    "rehype-autolink-headings": "^6.0.0",
    "rehype-slug": "^5.0.0",
    "remark-abbr": "^1.4.1",
    "rimraf": "^3.0.2",
    "stripe": "^8.174.0",
    "stylus": "^0.54.8",
    "svelte": "^3.29.0",
    "svelte-icons": "^2.1.0",
    "svelte-preprocess": "^4.0.0",
    "type-fest": "^2.0.0"
  }
}
