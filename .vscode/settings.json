{
  "spellright.language": ["en-US-9-0."],
  "spellright.documentTypes": ["markdown", "latex", "plaintext"],
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    ".eslintrc.cjs": false,
    ".gitignore": false,
    ".npmrc": false,
    ".prettierignore": false,
    ".prettierrc": false,
    ".vscode": false,
    "**/.svelte-kit": false,
    "**/build": false,
    "mdsvex.config.js": false,
    "node_modules": false,
    "pnpm-lock.yaml": false,
    "svelte.config.js": false,
    "tsconfig.json": false,
    ".nojekyll": false
  },
  "explorerExclude.backup": null,
  "search.exclude": {
    "**/.svelte-kit": true,
    "**/build": true
  },
  "editor.tabSize": 2,
  "editor.detectIndentation": false
}
