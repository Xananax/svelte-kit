{
  "spellright.language": ["en"],
  "spellright.documentTypes": ["markdown", "latex", "plaintext"],
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "**/.classpath": false,
    "**/.project": false,
    "**/.settings": false,
    "**/.factorypath": false,
    ".eslintrc.cjs": false,
    ".gitignore": false,
    ".npmrc": false,
    ".prettierignore": false,
    ".prettierrc": false,
    ".vscode": false,
    "**/.svelte-kit": false,
    "**/build": false,
    "mdsvex.config.js": false,
    "node_modules": false,
    "pnpm-lock.yaml": false,
    "svelte.config.js": false,
    "**/tsconfig.json": false,
    ".nojekyll": false,
    "build-scripts/**/*.js": false,
    "build-scripts/**/*.map": false
  },
  "explorerExclude.backup": null,
  "terminal.integrated.profiles.windows": {
    "cmd": {
      "path": "C:\\WINDOW\\System3d\\cmd.exe"
    }
  },
  "terminal.integrated.profiles.linux": {
    "bash (login)": {
      "path": "bash",
      "args": ["-l"]
    }
  },
  "search.exclude": {
    "**/.svelte-kit": true,
    "**/build": true
  },
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "cSpell.words": ["jekill", "lukeed", "mdsvex", "nojekyll", "oldschool", "rehype", "sveltejs"]
}
