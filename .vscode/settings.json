{
  "spellright.language": [
    "en"
  ],
  "spellright.documentTypes": [
    "markdown",
    "latex",
    "plaintext"
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    ".eslintrc.cjs": true,
    ".gitignore": true,
    ".npmrc": true,
    ".prettierignore": true,
    ".prettierrc": true,
    ".vscode": true,
    "**/.svelte-kit": true,
    "**/build": true,
    "mdsvex.config.js": true,
    "node_modules": true,
    "pnpm-lock.yaml": true,
    "svelte.config.js": true,
    "tsconfig.json": true,
    ".nojekyll": true
  },
  "explorerExclude.backup": null,
  "search.exclude": {
    "**/.svelte-kit": true,
    "**/build": true
  },
  "editor.tabSize": 2,
  "editor.detectIndentation": false
}
